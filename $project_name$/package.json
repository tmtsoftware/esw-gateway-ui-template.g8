{
  "name": "$project_name$",
  "version": "$version$",
  "license": "Apache License, Version 2.0",
  "private": true,
  "dependencies": {
    "@tmtsoftware/esw-ts": "$esw_ts_version$",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean fix",
    "test": "jest",
    "test:coverage": "jest --coverage ",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix --config .eslintrc.js './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "snowpack": "^2.11.1",
    "jest": "^26.4.2",
    "@types/jest": "^26.0.14",
    "typescript": "^4.0.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "npm-run-all": "^4.1.5",
    "@testing-library/react": "^11.0.4"
  }
}
