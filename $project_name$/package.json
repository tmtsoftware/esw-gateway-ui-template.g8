{
  "name": "$project_name$",
  "version": "$version$",
  "license": "Apache License, Version 2.0",
  "private": true,
  "dependencies": {
    "esw-ts":"^0.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean fix",
    "test": "jest",
    "test:coverage": "jest --coverage ",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix --config .eslintrc.js './**/*.{ts,tsx}' --max-warnings 2",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "snowpack": "^2.7.3",
    "jest": "^26.2.2",
    "@types/jest": "^26.0.8",
    "typescript": "^3.9.7",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "@testing-library/react": "^10.4.7"
  }
}
